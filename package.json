{
  "name": "ng-cloud",
  "version": "1.3.8",
  "description": "ng-zorro-antd admin panel front-end framework",
  "author": "topsec",
  "scripts": {
    "precommit": "npm run lint-staged",
    "ng": "ng",
    "start": "ng serve -o --host 192.168.33.55",
    "build": "node --max-old-space-size=5120 \"node_modules/@angular/cli/bin/ng\" build --prod --build--optimizer --aot",
    "encodebuild": "node --max-old-space-size=5120 \"node_modules/@angular/cli/bin/ng\" build --prod --build-optimizer --aot --configuration=encode",
    "buildnode": "node --max-old-space-size=8000 node_modules\\@angular\\cli\\bin\\ng build --prod --build--optimizer --aot",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "analyze": "npm run color-less && ng build --prod --build-optimizer --stats-json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "node scripts/color-less.js",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "release-major": "git pull origin tmp_update && standard-version -r major && git push --follow-tags origin tmp_update",
    "release-minor": "git pull origin tmp_update && standard-version -r minor && git push --follow-tags origin tmp_update",
    "release-patch": "git pull origin tmp_update && standard-version -r patch && git push --follow-tags origin tmp_update",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "hmr": "ng serve -c=hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/cdk": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/forms": "~8.0.1",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/router": "~8.0.1",
    "@antv/g2": "^3.5.7",
    "@delon/abc": "^8.9.0",
    "@delon/acl": "^8.9.0",
    "@delon/auth": "^8.9.0",
    "@delon/cache": "^8.9.0",
    "@delon/chart": "^8.9.0",
    "@delon/form": "^8.9.0",
    "@delon/mock": "^8.9.0",
    "@delon/theme": "^8.9.0",
    "@delon/util": "^8.9.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-datatable": "^15.0.1",
    "@w11k/ngx-componentdestroyed": "^4.1.4",
    "ajv": "^6.10.2",
    "angular-gridster2": "^7.2.0",
    "canvas": "^2.6.1",
    "canvg": "^2.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.9",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.6.0",
    "echarts-gl": "^1.1.1",
    "echarts-wordcloud": "^1.1.3",
    "editorconfig-tools": "^0.1.1",
    "html2canvas": "^1.0.0-alpha.12",
    "husky": "^1.3.1",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.2.1",
    "moment": "^2.24.0",
    "ng-alain": "^8.9.0",
    "ng-zorro-antd": "^8.0.0-rc.0",
    "ng2-dragula": "^2.1.1",
    "ng2-tree": "^2.0.0-rc.11",
    "ngx-echarts": "^4.2.1",
    "ngx-perfect-scrollbar": "^7.2.1",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "qrious": "^4.0.2",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.2",
    "screenfull": "~4.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "web-animations-js": "^2.3.2",
    "xlsx": "^0.15.6",
    "xmldom": "^0.1.27",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "babel-polyfill": "^6.26.0",
    "codelyzer": "^5.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "less-bundle-promise": "^1.0.7",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.5"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ],
    "ignore": [
      "src/assets/*"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
